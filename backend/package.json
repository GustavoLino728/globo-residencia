  "dependencies": {
    "@fastify/multipart": "^9.2.1",
    "fastify": "^5.6.1",
    "axios": "^1.6.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "p-queue": "^7.4.0"
  },
  "devDependencies": {
